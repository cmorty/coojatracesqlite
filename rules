#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

export ANT_OPTS+="-Dcooja_jar=/usr/share/cooja/cooja.jar"
export ANT_OPTS+="-Dmspsim_jar=/usr/share/cooja/apps/mspsim/mspsim.jar"
export ANT_OPTS+="-Dcooja_mspsim_jar=/usr/share/cooja/apps/mspsim/cooja_mspsim.jar"
export ANT_OPTS+="-Dcooja_lib=/usr/share/cooja"
export ANT_OPTS+="-Dcoojatrace=/usr/share/apps/coojatrace"
export ANT_OPTS+="-Dscala_lib=/usr/share/apps/coojatrace"
export ANT_OPTS+="-Darchive=/lib/coojatrace-sqlite.jar"

%:
	dh $@ 


